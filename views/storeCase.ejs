<% include header.ejs %>
<% include password.ejs %>
<% include navbar.ejs %>

<div class="container">

<form action="/<%=community%>/storeCaseAction" method="POST">

    <label class="control-label" for="Date">
      Date that Case Debuts (if calendar doesn't display in your browser, enter date in the format "yyyy-mm-dd")
    </label>
	<input type="date" name="Date"><br>
	
	<label class="control-label" for="FlexibleText1">
	Flexible Text 1 (Optional)
	</label>
	<textarea name="FlexibleText1" cols="100"></textarea><br>

	<label class="control-label" for="FlexibleText2">
	Flexible Text 2 (Optional)
	</label>
	<textarea name="FlexibleText2"></textarea><br>

	<label class="control-label" for="HPI">
	HPI
	</label>
	<textarea name="HPI"></textarea><br>

	<label class="control-label" for="PMH">
	PMH
	</label>
	<textarea name="PMH"></textarea><br>

	<label class="control-label" for="MedsAllergies">
	Meds Allergies
	</label>
	<textarea name="MedsAllergies"></textarea><br>

	<label class="control-label" for="SHxHRB">
	SHx, HRB
	</label>
	<textarea name="SHxHRB"></textarea><br>

	<label class="control-label" for="FHx">
	FHx
	</label>
	<textarea name="FHx"></textarea><br>

	<label class="control-label" for="PE">
	PE
	</label>
	<textarea name="PE"></textarea><br>

	<label class="control-label" for="LabsStudies">
	Labs and Studies
	</label>
	<textarea name="LabsStudies"></textarea><br>

	<label class="control-label" for="Imaging">
	Imaging
	</label>
	<textarea name="Imaging"></textarea><br>

	<label class="control-label" for="Citation">
	Citation for source of case
	</label>
	<input type="text" name="Citation"><br>

	<label class="control-label" for="Question">
	Question(s) that you'd like to ask the participants
	</label>
	<textarea name="Question"></textarea><br>

	<label class="control-label" for="Diagnosis">
	What is the diagnosis?
	</label>
	<input type="text" name="Diagnosis"><br>

	<label class="control-label" for="FurtherDetails">
	Further details about the patient not initially revealed in initial case presentation
	</label>
	<textarea name="FurtherDetails"></textarea><br>

	<label class="control-label" for="KeyPointsExplanation">
	Key points and explanation for this case
	</label>
	<textarea name="KeyPointsExplanation"></textarea><br>

	<input class="btn" type="submit">

</form>

</div>

<% include footer.ejs %>